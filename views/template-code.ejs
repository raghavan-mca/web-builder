        //     console.log('asd' + ($('#fmess').val()));      
        // }
        console.log($(this).attr('id'));
        if (this.value === undefined || this.value === '') {
            let errMess = `<div id="err_box"><div style="font-size: 13px; color: red;">Please Enter Data !!!</div></div>`;
            $('#submit_data').addClass('pn');
            $(this).parents('.bcard-frm-sec').children('#err_box').remove();
            $(this).parents('.bcard-frm-sec').append(errMess);
        } else if (this.value != undefined || this.value != '') {
            $('#submit_data').addClass('pn');
            $(this).parents('.bcard-frm-sec').children('#err_box').remove();

            if ($(this).attr('id') === 'femail') {

                if (regex.test(this.value) === false) {
                    let errMess = `<div id="err_box"><div style="font-size: 13px; color: red;">Please Enter Valid Email !!!</div></div>`;
                    $(this).parents('.bcard-frm-sec').children('#err_box').remove();
                    $(this).parents('.bcard-frm-sec').append(errMess);
                    $('#submit_data').addClass('pn');
                } else {
                    $(this).parents('.bcard-frm-sec').children('#err_box').remove();
                }
            } else if ($(this).attr('id') === 'pno') {

                if (regexPh.test(this.value) === false) {
                    let errMess = `<div id="err_box"><div style="font-size: 13px; color: red;">Please Enter Valid Phone Number !!!</div></div>`;
                    $(this).parents('.bcard-frm-sec').children('#err_box').remove();
                    $(this).parents('.bcard-frm-sec').append(errMess);
                    $('#submit_data').addClass('pn');
                } else {
                    $(this).parents('.bcard-frm-sec').children('#err_box').remove();
                }
            }

            if (($('#fn').val() != '' && $('#fn').val() != undefined) && ($('#pno').val() != '' && $('#pno').val() != undefined) && ($('#femail').val() != '' && $('#femail').val() != undefined && (regex.test($('#femail').val()) === true)) && ($('#fmess').val() != '' && $('#fmess').val() != undefined)) {
                console.log($('#fn').val());
                $('#submit_data').removeClass('pn');
            }
        }


    }

    $("input").on('keyup', checkValidation);
    $("textarea").on('keyup', checkValidation);


    function submitData(e) {
        let contactData = {
            first_name: $('#fn').val(),
            phone_number: $('#pno').val(),
            email: $('#femail').val(),
            message: $('#fmess').val()
        }
        let contactObj = {
            method: 'POST',
            body: JSON.stringify(contactData),
            headers: {
                'Content-Type': 'application/json'
            }
        }
        fetch($(e).attr('data-api'), contactObj)
            .then((res) => {
                res.json()
                    .then((res) => {
                        console.log(res);
                        console.log(res);
                    })
                    .catch((err) => {
                        console.error(err);
                    })
            })
            .catch((err) => {
                console.error(err);
            });
    }
    </script>
